<% @orders.each do |order| %>
<div class="row">
<h1> <%=  order.product.name %></h1>
 <p> <%= order.status %> | <%= order.quantity %> | <%= order.payment %> | <%= order.delivery %> | <%= order.user_id %> | <%= order.product_id %></p>
<h3> <%= order.user.first_name %>  <%= order.user.last_name %></h3>
</div>
<% end %>
